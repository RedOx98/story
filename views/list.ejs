<%- include("header") -%>


  <div class="box" id="heading">
    <h1> <%= ListTitle %> </h1>
  </div>
  
  <div class="box">
    <% for (var i=0; i<listItems.length; i++) { %>
      <div class="item">
        <input type="checkbox">
      <p><%= listItems[i] %></p>
      </div>
  <% } %>

  <form class="item" action="/" method="post">
    <input type="text" name="Todo" placeholder="New Item" autocomplete="off">
    <button type="submit" name="list" value=<%= ListTitle %>>+</button>
  </form>
  </div>
    

    
  <%- include("footer") -%>


  
